{
  "name": "assignexpert",
  "version": "0.1.0",
  "description": "monorepo for assignexpert",
  "main": "server.js",
  "scripts": {
    "start": "export NODE_ENV=production && node build/server.js",
    "dev": "export NODE_ENV=development && npm run build && node build/server.js",
    "clean": "npx rimraf build/",
    "copy-files": "npx copyfiles -u 1 \"src/web/**/*\" build/",
    "build": "npm run clean && npx tsc && npm run copy-files",
    "test": "export NODE_ENV=test && npx jest --coverage --passWithNoTests",
    "pretty": "npx prettier --write .",
    "lint": "npx eslint . --fix",
    "quality": "npm run lint && npm run pretty",
    "ci": "npm run quality && npm run test && npm run build",
    "docs": "npx apidoc -i src/routes/ -o docs"
  },
  "author": "assignexpert",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bullmq": "^1.89.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "pg": "^8.7.3",
    "redis": "^4.2.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.1",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "apidoc": "^0.52.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.20.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
